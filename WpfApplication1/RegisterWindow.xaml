<Window x:Class="WpfApplication1.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Registreer een nieuwe gebruiker" Height="267" Width="339">
    <Grid Margin="0,0,2,-1" Name="RegisterWindowLayoutRoot" >
        <TextBox Name="usernameTextbox" HorizontalAlignment="Left" Height="23" Margin="138,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Gebruikersnaam" HorizontalAlignment="Left" Margin="37,36,0,0" VerticalAlignment="Top"/>
        <Label HorizontalAlignment="Left" Margin="150,59,0,0" VerticalAlignment="Top"/>
        <Button IsDefault="true" Content="Registreren" HorizontalAlignment="Left" Margin="45,160,0,0" VerticalAlignment="Top" Width="213" Click="Button_Click"/>
        <TextBox IsReadOnly="True" Name="FeedbackTextblock" HorizontalAlignment="Left" Margin="45,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="70" Width="213"/>

    </Grid>
</Window>
